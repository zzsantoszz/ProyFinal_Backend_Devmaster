@using DAO.Modelo
@model tb_asunto_ticket

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Editar Tickets</h2>

@using (Html.BeginForm())
{
    @Html.HiddenFor(model => model.idAsunto_ticket)
    <div class="form-horizontal">
        <label>@Model.idAsunto_ticket</label>
        <div class="form-group">
            @Html.LabelFor(model => model.asunto_descripcion_ticket, new { @class = "control-label" })
            @Html.TextBoxFor(model => model.asunto_descripcion_ticket, htmlAttributes: new { @class = "form-control" })
            @*<input class="form-control" id="user_acceso" name="user_acceso" type="text" value="@Model.user_acceso"> *@
        </div>
        <div class="form-group">
            <input type="submit" value="Registrar" class="btn btn-success" />
        </div>
    </div>
}


