@using DAO.Modelo
@model tb_categoria_ticket

@{
    ViewBag.Title = "New";
    Layout = "~/Views/Shared/_Modal.cshtml";
}

<h2>Registrar Nueva Categoria</h2>

@using (Html.BeginForm())
{
<div class="form-horizontal">
    <div class="form-group">
        @Html.LabelFor(model => model.nombre_categoria_ticket, new { @class = "control-label" })
        @Html.TextBoxFor(model => model.nombre_categoria_ticket, htmlAttributes: new { @class = "form-control" })

    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.nombre_subcategoria_ticket, new { @class = "control-label" })
        @Html.TextBoxFor(model => model.nombre_subcategoria_ticket, htmlAttributes: new { @class = "form-control" })

    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.nombre_elemento_ticket, new { @class = "control-label" })
        @Html.TextBoxFor(model => model.nombre_elemento_ticket, htmlAttributes: new { @class = "form-control" })

    </div>
    <div class="form-group">
        <input type="submit" value="Registrar" class="btn btn-success" />
    </div>
</div>
}
